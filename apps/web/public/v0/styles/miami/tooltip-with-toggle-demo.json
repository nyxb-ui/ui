      "content": "import { Heart } from \"lucide-react\"\n\nimport { Toggle } from \"@/components/ui/toggle\"\nimport {\n   Tooltip,\n   TooltipContent,\n   TooltipProvider,\n   TooltipTrigger,\n} from \"@/components/ui/tooltip\"\n\nexport default function TooltipWithToggleDemo() {\n   return (\n      <TooltipProvider>\n         <Tooltip>\n            <TooltipTrigger>\n               <Toggle variant=\"outline\" aria-label=\"Toggle Heart\">\n                  <Heart className=\"size-4\" />\n               </Toggle>\n            </TooltipTrigger>\n            <TooltipContent>\n               <p>Heart</p>\n            </TooltipContent>\n         </Tooltip>\n      </TooltipProvider>\n   )\n}\n",
